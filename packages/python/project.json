{
  "root": "packages/python",
  "sourceRoot": "packages/python/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/python",
        "main": "packages/python/src/index.ts",
        "tsConfig": "packages/python/tsconfig.lib.json",
        "assets": [
          "packages/python/*.md",
          {
            "input": "./packages/python/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/python/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/python",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/python",
            "glob": "executors.json",
            "output": "."
          }
        ]      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/python/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/python"
      ],
      "options": {
        "jestConfig": "packages/python/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
